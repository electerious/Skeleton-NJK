{
  "name": "skeleton-njk",
  "version": "3.0.2",
  "description": "A front-end template build upon Rosid",
  "private": true,
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Skeleton.git"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "bower install",
    "start": "rosid serve src/ -o",
    "compile": "rosid compile src/ dist/ -i '**/_*' -i '**/.bower.json' -i '**/*.{data,config}.{js,json}'"
  },
  "dependencies": {
    "rosid": "^7.0.2",
    "rosid-handler-components": "^1.7.1",
    "rosid-handler-js": "^9.0.1",
    "rosid-handler-njk": "^4.0.0",
    "rosid-handler-sass": "^4.0.1"
  }
}